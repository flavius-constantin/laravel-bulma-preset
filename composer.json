{
    "name": "flavius-constantin/laravel-bulma-preset",
    "type": "library",
    "description": "Bulma Preset Scaffolding for Laravel 5.5",
    "keywords": ["preset", "bulma", "laravel"],
    "homepage": "https://github.com/flavius-constantin/laravel-bulma-preset",
    "license": "MIT",
    "authors": [
        {
            "name": "Flavius Constantin",
            "email": "me@flavius-constantin.de"
        }
    ],
    "require": {
        "laravel/framework": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "FlaviusConstantin\\BulmaPreset\\": "src/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "FlaviusConstantin\\BulmaPreset\\BulmaPresetServiceProvider"
       ]
      }
    }
}
